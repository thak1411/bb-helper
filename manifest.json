{
    "name": "BB Helper",
    "version": "1.0",
    "description": "BlackBoard Helper Made By Rn",
    "browser_action": {
        "default_popup": "view/popup.html"
    },
    "permissions": [ "activeTab" ],
    "content_scripts": [{
        "all_frames": true,
        "js": [ "/src/script.js" ],
        "matches": [ "<all_urls>" ],
        "run_at": "document_start"
    }],
    "manifest_version": 2
}